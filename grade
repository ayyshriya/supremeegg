#include<stdio.h>  
 int main()    
{    
  int m;
  char grade;
  printf("Enter marks");
  scanf("%d",&m);
  if(m>=85&&m<=100)
   grade='A';
  else if(m>=70&&m<=84)
   grade='B';
  else if(m>=55&&m<=55)
   grade='C';
  else if(m>=40&&m<=54)
   grade='D';
  else if(m<=40)
   grade='F';
  else
    printf("Invalid input");

  printf("Grade %c",grade);
  return 0;
}   
